<%- include('../includes/head.ejs') %>
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>
        <main>
            <% if (orders.length > 0) { %>
                <ul>
                    <% for (const order of orders) { %>
                        <li>
                            <h1># <%= order.id %></h1>
                            <ul>
                                <% for (const product of order.products) { %>
                                    <li>
                                        <%= product.title %> (<%= product.orderItem.quantity %>)
                                    </li>
                                <% } %>
                            </ul>
                        </li>
                    <% } %>
                </ul>
            <% } else { %>
                    <h1>Nothing there!</h1>
            <% } %>
        </main>
        <%- include('../includes/end.ejs') %>